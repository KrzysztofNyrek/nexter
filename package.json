{
  "name": "jagodawebpage",
  "version": "1.0.0",
  "description": "Simple website with some photo, text and link to YT video.",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "browser-sync start --server --files css/*.css --host 192.168.1.105",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dekstryn/JagodaWebPage.git"
  },
  "author": "Krzysztof Nyrek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dekstryn/JagodaWebPage/issues"
  },
  "homepage": "https://github.com/Dekstryn/JagodaWebPage#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "concat": "^1.0.3",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.25"
  },
  "dependencies": {}
}
